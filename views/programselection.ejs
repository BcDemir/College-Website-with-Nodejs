    <h2 style="text-align: center; margin-top: auto;">List of programs</h2>
    <table  class="table table-striped table-hover table-light">
      <thead class="thead-dark">
      <tr>
      <th>Course Name</th>
      <th>Course Section</th>
      <th>Course Time</th>
      <th>Course Fee</th>
      <th>Register</th>
      </tr></thead>
    <section data-position='programs'>
        <% for ( var i=0; i<allsubjects.length; i++) { %>
          <tbody><tr>
            <form action="/course/register" method="post" >
            <td><%= allsubjects[i].coursename %></td>
            <td><%= allsubjects[i].coursesection %></td>
            <td><%= allsubjects[i].coursetime %></td>
            <td><%= allsubjects[i].coursefee %></td>
            <td><input type="hidden" name="coursename" value="<%= allsubjects[i].coursename %>" />
            <input type="submit" value="Register" /></td> 
            </form>
          </tr></tbody>
        <% } %>      
    </section><br>
